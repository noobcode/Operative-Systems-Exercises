#include <stdio.h>
#include <stdlib.h>
#include <unistd.h>

// la prima printf viene persa perch√© non si effettua il flush dello stdout

int main (void) {
	int pid;
  printf("The quick brown fox jumped over ");
  //fflush(stdout);
  pid = fork();
  if(pid)
  	execl("/bin/echo","echo","the","lazy","dogs.",NULL);
  else
  	execl("/bin/echo","echo","fuck","the","system.",NULL);
  return 0;
}
